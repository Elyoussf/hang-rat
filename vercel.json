{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "main.js",
      "use": "@vercel/static"
    },
    {
      "src": "utils/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/utils/(.*)",
      "dest": "/utils/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}